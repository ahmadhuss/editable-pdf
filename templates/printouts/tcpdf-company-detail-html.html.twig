<div style="height: 15px;"></div>

<table border="0" cellpadding=0 cellspacing=0 width="100%" align="center"
       class="table_bl_border">
    <tr class=N_1 style="display:view;">
        <td class="pl" style="background:#cfcfcf;" colspan="4"><b>Officers
                Information </b></td>
    </tr>
</table>
<table border="0" cellpadding=0 cellspacing=0 width="100%" align="center"
       class="table_bl_border">
    {% set count = 1 %}
    {% for key, value in app.officers %}
        <tr>
            <td class="bl" colspan="2"> Officer {{ count }} -
                <b>{{ value.full_name|upper }}</b></td>
        </tr>
        <tr>
            <td class="pl x"> Full Name</td>
            <td style="width:75%;" class="tl"> {{ value.full_name|upper }}</td>
        </tr>
        <tr>
            <td class="pl x"> Former Full Name</td>
            <td style="width:75%;" class="tl"></td>
        </tr>
        <tr>
            <td class="pl x"> Normal Designation</td>
            <td style="width:75%;"
                class="tl"> {{ value.designation|upper }}</td>
        </tr>
        <tr>
            <td class="pl x"> ID Type</td>
            <td style="width:25%;" class="tc"> {{ value.id_type }}</td>
            <td class="pl x"> ID No.</td>
            <td style="width:25%;" class="tc"> {{ value.id_number }}</td>
        </tr>
        <tr>
            <td class="pl x"> Sex</td>
            <td style="width:25%;" class="tc"> {{ value.gender }}</td>
            <td class="pl x"> Birth</td>
            <td style="width:25%;"
                class="tc"> {{ value.dob }}</td>
        </tr>
        <tr>
            <td class="pl x"> Nationality</td>
            <td class="tc x"> {{ value.nationality }}</td>
            <td class="pl x"> Country of Birth</td>
            <td class="tc x"> {{ value.country_birth }}</td>
        </tr>
        <tr>
            <td class="pl x"> Address Line 1</td>
            <td style="width:75%;"
                class="tl"> {{ value.address_1 }}</td>
        </tr>
        <tr>
            <td class="pl x"> Address Line 2</td>
            <td style="width:75%;"
                class="tl"> {{ value.address_2 }}</td>
        </tr>
        <tr>
            <td class="pl x"> Postal</td>
            <td style="width:75%;"
                class="tl"> {{ value.postal_code }}</td>
        </tr>
        <tr>
            <td class="pl x"> Email</td>
            <td style="width:75%;" class="tl"> {{ value.email }}</td>
        </tr>
        <tr>
            <td class="pl x"> Mobile</td>
            <td style="width:75%;" class="tl"> {{ value.mobile }}</td>
        </tr>
        <tr>
            <td class="pl x"> Remark</td>
            <td style="width:75%;" class="tl"></td>
        </tr>
        <br>
        {% set count = count + 1 %}
    {% endfor %}
</table>
<div style="height: 15px;"></div>

<table border="0" cellpadding=0 cellspacing=0 width="100%" align="center"
       class="table_bl_border">
    <tr class=N_1 style="display:view;">
        <td class="pl" style="background:#cfcfcf;" colspan="4"><b>Capital
                Information </b></td>
    </tr>
</table>

<table border="0" cellpadding=0 cellspacing=0 width="100%" align="center"
       class="table_bl_border">
    <tr>
        <td class="pl" colspan="2"> Used Currency</td>
        <td class="tr"
            colspan="2"> {{ app.capital_currency }} </td>
    </tr>
    <tr>
        <td class="pl" colspan="2"> Start ~ End</td>
        <td class="tr" colspan="3"></td>
    </tr>
</table>
<br/><br/>
{% for key, value in app.capital_details %}
    <table border="0" cellpadding=0 cellspacing=0 width="100%" align="center"
           class="table_bl_border">
        <tr>
            <td class="tl" width="45%" height="20"><b> Class of shares </b></td>
            <td class="tc" width="18%" height="20"><b> Ordinary </b></td>
            <td class="tc" width="18%" height="20"><b> Preference </b></td>
            <td class="tc" width="19%" height="20"><b> Others </b></td>
        </tr>
        <tr>
            <td class="tl" width="45%" height="18"> Number of shares</td>


            {% if value.shares_class == "Ordinary" %}
                <td style="width:18%;"
                    class="tc">{{ value.no_issue_share|number_format(0,'.',',') }}</td>
            {% else %}
                <td style="width:18%;" class="tc"></td>
            {% endif %}

            {% if value.shares_class == "Preference" %}
                <td style="width:18%;"
                    class="tc">{{ value.no_issue_share|number_format(0,'.',',') }}</td>
            {% else %}
                <td style="width:18%;" class="tc"></td>
            {% endif %}


            {% if value.shares_class == "Others" %}
                <td style="width:18%;"
                    class="tc">{{ value.no_issue_share|number_format(0,'.',',') }}</td>
            {% else %}
                <td style="width:18%;" class="tc"></td>
            {% endif %}
        </tr>
        <tr>
            <td class="tl" width="45%" height="18"> Nominal amount of each
                share
            </td>


            {% if value.shares_class == "Ordinary" %}
                <td style="width:18%;"
                    class="tc">{{ value.each_share_price|number_format(1,'.',',') }}</td>
            {% else %}
                <td style="width:18%;" class="tc"></td>
            {% endif %}

            {% if value.shares_class == "Preference" %}
                <td style="width:18%;"
                    class="tc">{{ value.each_share_price|number_format(1,'.',',') }}</td>
            {% else %}
                <td style="width:18%;" class="tc"></td>
            {% endif %}

            {% if value.shares_class == "Others" %}
                <td style="width:18%;"
                    class="tc">{{ value.each_share_price|number_format(1,'.',',') }}</td>
            {% else %}
                <td style="width:18%;" class="tc"></td>
            {% endif %}

        </tr>
        <tr>
            <td class="tl" width="45%" height="18"> Amount paid or due and
                payable on each share paid due and payable
            </td>

            <td style="width:18%;"
                class="tc"></td>
            {#            <td style="width:18%;" #}
            {#                class="tc"> {{ datas.paid_amount|number_format(0,'.',',') }} </td> #}
            <td style="width:18%;"
                class="tc"> {{ value.field_share_paid_due_and_payable.field_others }} </td>
            <td style="width:18%;"
                class="tc"> {{ value.field_share_paid_due_and_payable.field_preference }} </td>
        </tr>
        {#        <tr> #}
        {#          <td style="width:18%;" class="tc"> {{ value.field_paid_or_due_on_each.field_ordinary }} </td> #}
        {#          <td style="width:18%;" class="tc"> {{ value.field_paid_or_due_on_each.field_others }} </td> #}
        {#          <td style="width:18%;" class="tc"> {{ value.field_paid_or_due_on_each.field_preference }} </td>          #}
        {#        </tr> #}
        <tr>
            <td class="tl" width="45%" height="18"> Amount of premium paid or
                payable on each Share
            </td>
            <td style="width:18%;"
                class="tc"> {{ value.field_paid_or_due_on_each.field_ordinary }} </td>
            <td style="width:18%;"
                class="tc"> {{ value.field_paid_or_due_on_each.field_others }} </td>
            <td style="width:18%;"
                class="tc"> {{ value.field_paid_or_due_on_each.field_preference }} </td>
        </tr>
        <tr>
            <td class="tc" colspan="4"> The consideration for which the shares
                have been so allotted is as follows:<br/> N/A
            </td>
        </tr>
    </table>
    <br/><br/>
{% endfor %}
<div style="height: 15px;"></div>
</body>
