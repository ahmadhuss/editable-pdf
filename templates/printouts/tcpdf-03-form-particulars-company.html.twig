<style>

  .tl {
    height:250px;
    line-height:20px;
    padding-left: 20px;
  }

</style>
<h1 style="width:100%;text-align:center;">FORM OF PARTICULARS</h1>

<div style="width:100%; height:10px;"></div>
<h1 style="width:100%;text-align:left;font-size:17px;">NAME OF COMPANY : {{ datas.company_name }} {{ datas.field_company_type }}</h1>
<div style="width:100%; height:10px;"></div>

<table border="1" width="100%" style="padding-left: 10px; padding-right: 10px; padding-top: 10px; padding-bottom: 10px;">
  <tr>
    <td class="tl" width="40%" height="40" valign="absmiddle">  1. Company Name in Full</td>
    <td class="tl" width="60%" height="40" valign="absmiddle"><b>{{ datas.officer.field_full_name }}</b></td>
  </tr>
  <tr>
    <td class="tl" width="40%" height="40">  2. Date of Incorporation</td>
    <td class="tl" width="60%" height="40">{{ datas.officer.field_date_of_birth|date('d/m/Y') }}</td>
  </tr>
  <tr>
    <td class="tl" width="40%" height="40">  3. Country of Incorporation</td>
    <td class="tl" width="60%" height="40">{{ datas.officer.field_country_of_birth }}</td>
  </tr>
  <tr>
    <td class="tl" width="40%" height="40">  4. {{ datas.officer.field_id_type }}</td>
    <td class="tl" width="60%" height="40">{{ datas.officer.field_id_no|upper }}</td>
  </tr>
  <tr>
    <td class="tl" width="40%" height="20"  style="line-height:120%;word-wrap:break-word">  5. Registered Address</td>
    <td class="tl" width="60%" height="20">{{ datas.officer.field_address_line_1 }} {{ datas.officer.field_address_line_2 }} {{ datas.officer.field_postal_code }}</td>
  </tr>
  <tr>
    <td class="tl" width="40%" height="40">  6. Designation in above Company</td>
    <td class="tl" width="60%" height="40">{{ datas.officer.field_position|upper}}</td>
  </tr>
  <tr>
    <td class="tl" width="40%" height="40">  7. E-mail Address</td>
    <td class="tl" width="60%" height="40" style="line-height:120%;word-wrap:break-word">{{ datas.officer.field_email_id|upper }}</td>
  </tr>
  <tr>
    <td class="tl" width="40%" height="40">  8.  Contact No.</td>
    <td class="tl" width="60%" height="40">{{ datas.officer.field_mobile }}</td>
  </tr>
  <tr>
    <td width="100%"  align="left" colspan="3" style="padding: 0px" >
      <table border="0" width="100%" align="center">
        <tr height="40"><td></td></tr>
        <tr height="40"><td></td></tr>
        <tr><td width="90%" height="40" align="center" style="padding:10px;"></td></tr>
        <tr><td width="90%" height="20" align="left" style="padding:10px;"></td></tr>
      </table>

    </td>
  </tr>
</table>

<table border="0" width="100%">
  <tr>
    <td width="90%" height="30" align="center" style="padding:10px;"></td>
  </tr>
</table>
<br/>

<table border="0" width="100%">
  <tr>
    <td>
      <div style="width:100%; height:50px;"></div>
    </td>
  </tr>
  <tr>
    <td class="tl" width="50%" height="60" align="center" style="line-height:120%;word-wrap:break-word;">____ / ____ / ________<br>Date</td>
    <td class="tl" width="50%" height="90" align="center" style="line-height:120%;">
      ____<span style="color: white;font-size: 0.1px;">{{ datas.signature }}</span>___________<br>Signature
    </td>
  </tr>
</table>
