<html>
  <body>
    {% for directors in app.officers_directors %}
      <h2 style="width:100%;text-align:center;">REGISTER OF DIRECTORS / ALTERNATE DIRECTORS</h2>
      <div style="width:100%; height:10px;"></div>
      <table>
        <tr>
          <td style="text-align:left;font-size:11px; font-weight: bold;">Name of Company:&nbsp;&nbsp;&nbsp;&nbsp;{{ app.company_name }}</td>
          <td style="text-align:right;font-size:11px;">Page {{ loop.index }}</td>
        </tr>
      </table>
      <div style="width:100%; height:5px;"></div>
      <table cellpadding="4" border="3" width="100%">
        <thead>
        <tr style="text-align:center;font-size:11px; font-weight: bold;">
          <td class="tl" style="border: 1px solid #111" width="35%"><br/><br/>Full name and residential Address</td>
          <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Identity / Passport no.</td>
          <td class="tl" style="border: 1px solid #111" width="15%"><br/><br/>Nationality</td>
          <td class="tl" style="border: 1px solid #111" width="10%"><br/>Date of appointment or change</td>
          <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Date ceased as director</td>
          <td class="tl" style="border: 1px solid #111" width="20%">Particulars of other directorship of public companies or subsidiaries of public companies incorporated in Singapore</td>
        </tr>
        </thead>
        <tbody>
          <tr style="font-size: 11px">
            <td class="tl" width="35%" style="border: 1px solid #111;">{{ directors.full_name }}</td>
            <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">{{ directors.id_number }}</td>
            <td class="tl" width="15%" style="text-align:center; border: 1px solid #111">{{ directors.nationality }}</td>
            <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">
              {{ directors.date_appointed }}
            </td>
            <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">
              {{ directors.date_ceased }}
            </td>
            <td class="tl" width="20%" style="border: 1px solid #111">{% if directors.date_entry %}Change of director's particulars on {{ directors.date_entry }}{% endif %}
            </td>
          </tr>
          <tr style="font-size: 11px">
            <td class="tl" style="border: 1px solid #111">{{ directors.address_1 }}</td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
          </tr>
          <tr style="font-size: 11px">
            <td class="tl" style="border: 1px solid #111">{{ directors.address_2 }} {% if directors.postal_code %}({{ directors.postal_code }}){% endif %}</td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
          </tr>
          <tr>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
            <td class="tl" style="border: 1px solid #111"></td>
          </tr>
        </tbody>
      </table>
      {% if loop.last %}
      {% else %}
        <br pagebreak="true" />
      {% endif %}
    {% endfor %}
  </body>
</html>
