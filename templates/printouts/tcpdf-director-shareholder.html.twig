{#{{ kint(datas) }}#}
<html>
  <style>
    .tl { height:250px; line-height:20px; padding-left: 20px; }
  </style>
  <body>
    <h1 style="width:100%;text-align:center;">FORM OF PARTICULARS</h1>

    <div style="width:100%; height:10px;"></div>
    <div style="width:100%;text-align:center;font-size:14px;">(Pursuant to Section 173 of the Companies Act, Cap. 50)</div>
    <div style="width:100%; height:30px;"></div>
    <h1 style="width:100%;text-align:left;font-size:17px;">NAME OF COMPANY : {{ datas.node.title }}</h1>
    <div style="width:100%; height:30px;"></div>

    <table border="1" width="100%" style="padding-left: 10px; padding-right: 10px; padding-top: 10px; padding-bottom: 10px;">
      <tr>
        <td class="tl" width="40%" height="40" valign="absmiddle">  1. Name in full (as appear in the passport)</td>
        <td class="tl" width="60%" height="40" valign="absmiddle"><b>{{ datas.node.field_director_shareholder[datas.uid].field_full_name }}</b></td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="40">  2. Former full name (if any)</td>
        <td class="tl" width="60%" height="40"></td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="40">  3. Date / Country of Birth</td>
        <td class="tl" width="60%" height="40">{{ datas.node.field_director_shareholder[datas.uid].field_address_line_1 }} / {{ datas.node.field_director_shareholder[datas.uid].field_country_of_birth }}</td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="40">  4. Nationality / Citizenship</td>
        <td class="tl" width="60%" height="40">{{ datas.node.field_director_shareholder[datas.uid].field_nationality }}</td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="20"  style="line-height:120%;word-wrap:break-word">  5. NRIC / FIN / Passport No.<br/>&nbsp;&nbsp;&nbsp;(Please attach a copy of the passport)</td>
        <td class="tl" width="60%" height="20">{{ datas.node.field_director_shareholder[datas.uid].field_id_type }} {{ datas.node.field_director_shareholder[datas.uid].field_id_no }}</td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="40">  6. Business Occupation</td>
        <td class="tl" width="60%" height="40">
          {% set position = '' %}
          {% for key, value in datas.node.field_director_shareholder[datas.uid].field_position %}
            {% set position = position ~ value ~ '/' %}
          {% endfor %}
          {{ position }}
        </td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="40">  7. Residential Address</td>
        <td class="tl" width="60%" height="40" style="line-height:120%;word-wrap:break-word">{{ datas.node.field_director_shareholder[datas.uid].field_address_line_1 }} {{ datas.node.field_director_shareholder[datas.uid].field_address_line_2 }} {{ datas.node.field_director_shareholder[datas.uid].field_postal_code }}</td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="40">  8. Designation in above named Company</td>
        <td class="tl" width="60%" height="40">{{ position }}</td>
      </tr>
      <tr>
        <td class="tl" width="40%" height="40">  9. E-mail Address and Mobile No.</td>
        <td class="tl" width="60%" height="40" style="line-height:120%;word-wrap:break-word"><br/>{{ datas.node.field_director_shareholder[datas.uid].field_email_id }}<br/>{{ datas.node.field_director_shareholder[datas.uid].field_work_phone }}</td>
      </tr>
      <tr>
        <td width="100%" height="140" align="left" colspan="3" style="padding-top: 10px;">
          <p>  10.	*** Particulars of other Directorships : </p>

          <table border="0" width="100%" align="center">
            <tr height="40">
              <td width="33.3333%" align="center" style="font-size:12px;"> <u>Name of Company</u> </td>
              <td width="33.3333%" align="center" style="font-size:12px;"> <u>Company No.</u> </td>
              <td width="33.3333%" align="center" style="font-size:12px;"> <u>Date of Appointment</u> </td>
            </tr>
          </table>

        </td>
      </tr>
    </table>

    <table border="0" width="100%">
      <tr>
        <td width="90%" height="5" align="center" style="padding:10px;"></td>
      </tr>
      <tr>
        <td width="90%" height="20" align="left" style="padding:10px;">
          * Apply only to directorships in public companies or subsidiaries of public companies in Singapore. If space is insufficient, please use annexes.
        </td>
      </tr>
    </table> 
    <br/>
    <br/>
    <br/>

    <table border="0" width="100%">
      <tr>
        <td>
          <div style="width:100%; height:80;"></div>
        </td>
      </tr>
      <tr>
        <td class="tl" width="50%" height="90" align="center" style="line-height:120%;word-wrap:break-word;"> ____ / ____ / ________<br>Date</td>
        <td class="tl" width="50%" height="90" align="center" style="line-height:120%;word-wrap:break-word;"> _______________<br>Signature</td>
      </tr>
    </table>

  </body>
</html>