    <td align="left" style="width:30%;"><img src="../public/assets/printouts/leekim-service.png"
                                             style="height:40px;"></td>
    <td align="right" style="width:70%;"><h1
        style="font-size:20px;">{{ app.company_name }}</h1></td>
  </tr>
