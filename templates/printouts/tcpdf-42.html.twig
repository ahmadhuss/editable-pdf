<html>
  <body>
    {% if datas.secretaries|length > 0 %}
      {% for secretarie in datas.secretaries %}
        <h2 style="width:100%;text-align:center;">REGISTER OF SECRETARIES AND MANAGERS</h2>
        <div style="width:100%; height:10px;"></div>
        <table>
          <tr>
            <td style="text-align:left;font-size:11px; font-weight: bold;">Name of Company:&nbsp;&nbsp;&nbsp;&nbsp;{{ datas.company_name }} {{ datas.field_company_type }}</td>
            <td style="text-align:right;font-size:11px;">Page {{ loop.index }}</td>
          </tr>
        </table>
        <div style="width:100%; height:5px;"></div>
        <table cellpadding="4" border="3" width="100%">
          <tr>
            <td colspan="6" style="border: 1px solid #111; text-align: center;">SECRETARIES</td>
          </tr>
          <tr style="text-align:center;font-size:11px; font-weight: bold;">
            <td class="tl" style="border: 1px solid #111" width="20%"><br/><br/>Full name</td>
            <td class="tl" style="border: 1px solid #111" width="35%"><br/><br/>Residential address</td>
            <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Identity / Passport no.</td>
            <td class="tl" style="border: 1px solid #111" width="15%"><br/>Nationality</td>
            <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Date of appointment or change</td>
            <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Date ceased as secretary</td>
          </tr>
          {% if secretarie.se %}
            {% for office in secretarie.se %}
              <tr style="font-size: 11px">
                <td class="tl" width="20%" style="text-align:left;border: 1px solid #111;">{{ office.field_full_name }}</td>
                <td class="tl" width="35%" style="border: 1px solid #111">{{ office.field_address_line_1 }}</td>
                <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">{{ office.field_id_no }}</td>
                <td class="tl" width="15%" style="text-align:center; border: 1px solid #111">{{ office.field_nationality }}</td>
                <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">
                  {% if office.field_date_appointed_registrable %}
                    {{ office.field_date_appointed_registrable|date('d/m/Y') }}
                  {% endif %}
                </td>
                <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">
                  {% if office.field_date_ceased_as_registrable %}
                    {{ office.field_date_ceased_as_registrable|date('d/m/Y') }}
                  {% endif %}
                </td>
              </tr>
              <tr style="font-size: 11px">
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111">{{ office.field_address_line_2 }} {% if office.field_postal_code %}({{ office.field_postal_code }}){% endif %}</td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
              </tr>
              <tr>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
              </tr>
            {% endfor %}
          {% else %}
            <tr>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
            </tr>
          {% endif %}
          <tr>
            <td colspan="6" style="border: 1px solid #111; text-align: center;">MANAGER</td>
          </tr>
          <tr style="text-align:center;font-size:11px; font-weight: bold;">
            <td class="tl" style="border: 1px solid #111" width="20%"><br/><br/>Full name</td>
            <td class="tl" style="border: 1px solid #111" width="35%"><br/><br/>Residential address</td>
            <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Identity / Passport no.</td>
            <td class="tl" style="border: 1px solid #111" width="15%"><br/><br/>Nationality</td>
            <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Date of appointment or change</td>
            <td class="tl" style="border: 1px solid #111" width="10%"><br/><br/>Date ceased as secretary</td>
          </tr>
          {% if secretarie.mana %}
            {% for office in secretarie.mana %}
              <tr style="font-size: 11px">
                <td class="tl" width="20%" style="text-align:left;border: 1px solid #111;">{{ office.field_full_name }}</td>
                <td class="tl" width="35%" style="border: 1px solid #111">{{ office.field_address_line_1 }}</td>
                <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">{{ office.field_id_no }}</td>
                <td class="tl" width="15%" style="text-align:center; border: 1px solid #111">{{ office.field_nationality }}</td>
                <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">
                  {% if office.field_date_appointed_registrable %}
                    {{ office.field_date_appointed_registrable|date('d/m/Y') }}
                  {% endif %}
                </td>
                <td class="tl" width="10%" style="text-align:center; border: 1px solid #111">
                  {% if office.field_date_ceased_as_registrable %}
                    {{ office.field_date_ceased_as_registrable|date('d/m/Y') }}
                  {% endif %}
                </td>
              </tr>
              <tr style="font-size: 11px">
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111">{{ office.field_address_line_2 }} {% if office.field_postal_code %}({{ office.field_postal_code }}){% endif %}</td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
              </tr>
              <tr>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
                <td class="tl" style="border: 1px solid #111"></td>
              </tr>
            {% endfor %}
          {% else %}
            <tr>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
              <td class="tl" style="border: 1px solid #111"></td>
            </tr>
          {% endif %}
        </table>
        {% if loop.last %}
        {% else %}
          <br pagebreak="true" />
        {% endif %}
      {% endfor %}
    {% endif %}
  </body>
</html>
